
<span class="muted b">Start:</span>
<input type="hidden" size="10" name="date_start" id="date_start" value="<%= dr.date_start %>">
<input type="text" size="10" id="date_start_picker" placeholder="Fecha inicial" value="<%= dr.date_start %>">

<span class="muted b">End:</span>

<input type="hidden" size="10" name="date_end" id="date_end" value="<%= dr.date_end %>">
<input type="text" size="10" id="date_end_picker" placeholder="Fecha final" value="<%= dr.date_end %>">

<script type="text/javascript">
  $(function() {
    $('#date_start_picker').datepicker({
      yearRange: '1900:',
      showOn: 'both',
      altFormat: 'yy-mm-dd',
      altField: $('#date_start')
    });
    $('#date_end_picker').datepicker({
      yearRange: '1900:',
      showOn: 'both',
      altFormat: 'yy-mm-dd',
      altField: $('#date_end')
    });
  });
</script>
