
<% @page_title = "Chart of Accounts" %>

<% content_for :links do %>
  <div class="btn-group" role="group">
    <%= link_to 'New Account...', new_account_path, :class => 'new' %>
  </div>
<% end %>




  
<table class="table">
  <tr>
    <th>Name</th>
  </tr>

<% @accounts.each do |account| %>
  <tr>
    <td><%= account.name %>
    <td><%= account.active  %>
    <td><%= account.subtype %>
    <td><%= account.currency  %>
    <td><%= account.description  %>
    <td><%= link_to account.name, account %>
  </tr>
<% end %>
</table>

