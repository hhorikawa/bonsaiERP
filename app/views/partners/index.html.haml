- content_for :links do
  %ul.breadcrumbs.span6
    %li
      =link_to 'Contactos', contacts_path


.row-fluid
  .span6
    %h1 Business Partners
  .span6.r
    = link_to 'Export', contacts_path(format: 'xls'), class: 'btn'

    %tags(model='Contact' tag-ids='#{ tag_ids }' url='#{ contacts_path }')
    = link_to "New Business Partner...", new_contact_path, class: 'btn btn-secondary'

= render 'search'

- paginate_html = paginate(@partners)

= paginate_html

%ul.unstyled.listing.has-row-check
  = render @partners

= paginate_html
